{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*",
        "./hooks/*",
        "./components/*",
        "./services/*",
        "./contexts/*",
        "./types/*",
        "./utils/*",
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
