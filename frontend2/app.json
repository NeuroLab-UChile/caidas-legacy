{
  "expo": {
    "name": "We Train",
    "slug": "caidas-we-flow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_color.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo_color.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "La aplicación necesita acceso a tus fotos para subir imágenes de perfil."
      },
      "bundleIdentifier": "com.cica.wetrain"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_color.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MEDIA_LIBRARY",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET"
      ],
      "package": "com.cica.wetrain"
    },
    "web": {
      "favicon": "./assets/images/logo_color.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación necesita acceso a tus fotos para subir imágenes de perfil.",
          "cameraPermission": "La aplicación necesita acceso a la cámara para tomar fotos de perfil."
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": false
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "scheme": "wetrain",
    "developmentClient": {
      "silentLaunch": true
    },
    "extra": {
      "disableHotReloadR": true,
      "apiUrl": "https://caidas.uchile.cl/api",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a7f19b89-500b-4ffd-b403-17764f59ce07"
      }
    },
    "platforms": [
      "android",
      "web"
    ],
    "owner": "juanovenegas"
  }
}
